%% draw the obstacle circle
r = 1.5;
circle_xy = [10 7];
index = 0;
for i = 0:0.01:3*pi
    index = index+1;
    circle_x(index) = 10 + r*cos(i);
    circle_y(index) = 7 +  r*sin(i);
end

%% plot the final trajectory with robotic arm
figure()
hold on
plot(circle_x,circle_y,'-')
plot(x_displacement,y_displacement)
[Q1,Q2] = two_arm_IK(14,,true);
plot_robot(Q1,Q2);
pbaspect([14 8.5 1])
axis([0 14 0 8.5])
xlabel('x / m')
ylabel('y / m')
title('Elbow up reaching point C')

